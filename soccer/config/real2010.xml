<config>
	<motionModule>
		<robot>
			<axles>
	            <axle x="1" y="1" />
	            <axle x="1" y="-1" />
	            <axle x="-1" y="-1" />
	            <axle x="-1" y="1" />
        	</axles>
		</robot>
	</motionModule>
	
	<!-- Map from robot types to revision -->
	<revLUT>
		<rev2010>
			<robot id="0"/>
			<robot id="1"/>
			<robot id="2"/>
			<robot id="3"/>
			<robot id="4"/>
			<robot id="5"/>
			<robot id="6"/>
			<robot id="7"/>
			<robot id="8"/>
			<robot id="9"/>
			<robot id="10"/>
			<robot id="11"/>
			<robot id="12"/>
			<robot id="13"/>
			<robot id="14"/>
			<robot id="15"/>
		</rev2010>
	</revLUT>
	
	<!-- Robot configuration information -->
	<robots>
	
		<!-- Default 2010 robot -->
	    <robot id="2010" rev="rev2010">
	    	<!-- done -->      
	    	<motion>
		        <!-- calibration based data -->
		        <translation>
			    	<deg value="0">
						<dynamics velocity="2.21557003571" acceleration="5.14073707537" deceleration="1.92777640326375" />
					</deg>
					<deg value="45">
						<dynamics velocity="1.85250468293" acceleration="4.76765854021" deceleration="1.78787195257875" />
					</deg>
				</translation>
				
				<!-- rotational dynamics -->
				<rotation>
					<dynamics velocity="100" acceleration="0" deceleration="0" />
					
					<pid type="angle" p=".4" i="0" d="0" />
				</rotation>
				
				<!-- wheel PID controller -->
				<control>
					<pid type="wheel" p="1.0" i="0" d="0" />
				</control>
				
				<!-- output filtering -->
				<coeffs>
					<coeff value="10.0"/>
					<coeff value="5.0"/>
					<coeff value="2.0"/>
					<coeff value="0.5"/>
				</coeffs>
			</motion>
			<kicker m="10" b="0" />
	    </robot>
	    
	</robots>
	
	<worldModel>
		<pos>
	    	<filter alpha="1" beta="0" gamma="0" />
	    </pos>
	    <angle>
	    	<filter alpha="1" beta="0" gamma="0" />
	    </angle>
	    
	    <ball>
	    </ball>
	</worldModel>
</config>
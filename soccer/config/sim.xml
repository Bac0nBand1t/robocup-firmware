
<motion>

    <linearController name="pos">
        <p value="10.0" />
        <i value="0" />
        <d value="0" />
        <windup value="10" />
    </linearController>

    <linearController name="angle">
        <p value=".05" />
        <i value="0" />
        <d value="0" />
        <windup value="10" />
    </linearController>

    <robotData>
        <!-- Wheels
            /3 ^ 0\
                ^
            \2 ^ 1/

            Axels are specified on robot coord system
        -->
        <axels>
            <axel x="1" y="1" />
            <axel x="1" y="-1" />
            <axel x="-1" y="-1" />
            <axel x="-1" y="1" />
        </axels>
        <!-- The max acceleration is based on a delta_t of 11ms !-->
        <maxAccel value = "100"/>
        <maxWheelVel value = "120"/>
    </robotData>

</motion>

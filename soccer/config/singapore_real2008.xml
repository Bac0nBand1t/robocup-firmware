<config>
	<motionModule>
		<robot>
			<axles>
	            <axle x="1" y="1" />
	            <axle x="1" y="-1" />
	            <axle x="-1" y="-1" />
	            <axle x="-1" y="1" />
        	</axles>
		</robot>
	</motionModule>
	
	<!-- Map from robot types to revision -->
	<revLUT>
		<rev2008>
			<robot id="0"/>
			<robot id="1"/>
			<robot id="2"/>
			<robot id="3"/>
			<robot id="4"/>
			<robot id="5"/>
			<robot id="6"/>
			<robot id="7"/>
			<robot id="8"/>
			<robot id="9"/>
			<robot id="10"/>
			<robot id="11"/>
			<robot id="12"/>
			<robot id="13"/>
			<robot id="14"/>
			<robot id="15"/>
		</rev2008>
	</revLUT>
	
	<!-- Robot configuration information -->
	<robots>
	     
		<!-- Default 2008 robot -->	  
	    <robot id="2008" rev="rev2008">	     
	    	<motion>
		        <!-- calibration based data -->
		        <translation>
			    	<deg value="0">
						<dynamics velocity="1.2" acceleration="5.0" deceleration="0.5" />
					</deg>
					<deg value="45">
						<dynamics velocity="1.5" acceleration="5.0" deceleration="0.5" />
					</deg>
				</translation>
				
				<!-- rotational dynamics -->
				<rotation>
					<dynamics velocity="100" acceleration="0" deceleration="0" />
					
					<pid type="angle" p=".4" i="0" d="0" />
				</rotation>
								
				<!-- output filtering -->
				<coeffs>
					<coeff value="10.0"/>
					<coeff value="0.5"/>
					<coeff value="0.0"/>
					<coeff value="0.0"/>
				</coeffs>
			</motion>
			
			<kicker m="20" b="0" />
	    </robot>
	    
	</robots>
	
	<worldModel>
		<pos>
	    	<filter alpha="1" beta="0" gamma="0" />
	    </pos>
	    <angle>
	    	<filter alpha="1" beta="0" gamma="0" />
	    </angle>
	    
	    <ball>
	    </ball>
	</worldModel>
</config>